//size

int r,g,b;
void setup(){
  size( 640,480);
  r=  100;
  g= 200;
  b= 255;
}
///button
void draw() {
  background ( r, g, b );
  showButton( button1X, button1Y, button1W, button1H);
   text("Press me",  button1X+button1W/2, button1Y+button1H*1/2 );
  fill(50,150);
  rect(20,50,100,200);
}


////// click
void mousePressed(){
  if ( hit(mouseX,mouseY, r, g, b))

} 
